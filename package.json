{
  "name": "lime-sso",
  "version": "0.0.1",
  "description": "sso service",
  "main": "./src/index",
  "repository": "https://github.com/N1MBER/lime-sso.git",
  "author": "N1MBER <socialfox33@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "stylelint-fix": "stylelint --fix",
    "tsc-dry-run": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.tsx,.ts --fix . --quiet --fix",
    "lint:css": "stylelint ./src/**/*.css --fix",
    "lint": "yarn run lint:js && yarn run lint:css",
    "format:svg": "prettier --write '**/*.svg' --parser html",
    "pre-push": "rm -rf ./dist",
    "pre-commit": "yarn run lint-staged",
    "test": "rm -rf ./dist && yarn tsc-dry-run",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "build_dev": "cross-env NODE_ENV=production webpack --env SERVER=dev --config webpack.config.js",
    "format": "npx prettier ./src --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "npm run lint",
      "bash -c 'tsc --noEmit --noEmitOnError --pretty'"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": true,
      "types": {
        "feat": {
          "description": "Adding new feature"
        },
        "fix": {
          "description": "Bug fix"
        },
        "refactor": {
          "description": "Editing code without fixing bugs or adding new features"
        },
        "chore": {
          "description": "Add / update / customize tools and libraries"
        }
      }
    }
  },
  "peerDependencies": {},
